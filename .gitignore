cat > .gitignore << 'EOF'
# Python
__pycache__/
*.pyc
*.pyo
*.pyd
.Python
*.so
.pytest_cache/

# Virtual environment
venv/
env/

# IDE
.vscode/
.idea/

# Logs
logs/*.log
logs/automation/*.log

# Test artifacts
tests/screenshots/*.png
tests/results/*.html
tests/automation/temp/*

# Database
data/bot.db-journal

# OS
.DS_Store
Thumbs.db

# Environment
.env.local
.env.production

# Temporary files
*.tmp
*.temp
EOF